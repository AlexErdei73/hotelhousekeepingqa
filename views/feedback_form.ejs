<%- include("_header") %>
    <h1><%= title %></h1>
    <form action="" method="POST">
        <div class="form-group">
            <label for="roomnumber" class="form-label">Roomnumber*</label>
            <input type="number" min="0" max="9999" name="roomnumber" id="roomnumber" class="form-control" required value="<%- feedback ? feedback.roomnumber : '' %>">
        </div>
        <div class="form-group">
            <label for="checkin_date" class="form-label">Checkin Date*</label>
            <input type="date" name="checkin_date" id="checkin_date" class="form-control" required value="<%- feedback ? feedback.checkin_date : '' %>">
        </div>
        <div class="form-group">
            <label for="checkout_date" class="form-label">Checkout Date*</label>
            <input type="date" name="checkout_date" id="checkout_date" class="form-control" required value="<%- feedback ? feedback.checkout_date : '' %>">
        </div>
        <div class="form-group">
            <label for="feedback_date" class="form-label">Feedback Date*</label>
            <input type="date" name="feedback_date" id="feedback_date" class="form-control" required value="<%- feedback ? feedback.feedback_date : '' %>">
        </div>
        <div class="form-group">
            <label for="score" class="form-label">Feedback Score*</label>
            <input type="number" min="1" max="10" name="score" id="score" class="form-control" required value="<%- feedback ? feedback.score : '' %>">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
<%- include("_formerrors") %> 
<%- include("_footer") %> 